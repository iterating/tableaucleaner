{
  "cleaningRules": {
    "removeDuplicates": {
      "type": "deduplication",
      "enabled": false,
      "parameters": {
        "columns": {
          "type": "array",
          "required": true
        }
      },
      "columns": []
    },
    "handleMissingValues": {
      "type": "missing_values",
      "enabled": false,
      "parameters": {
        "method": {
          "type": "string",
          "options": ["mean", "median", "mode"]
        },
        "columns": {
          "type": "array",
          "required": true
        }
      },
      "method": "mean",
      "columns": []
    },
    "normalizeBloodSugarLevels": {
      "type": "normalization",
      "enabled": false,
      "parameters": {
        "minValue": {
          "type": "number",
          "required": true
        },
        "maxValue": {
          "type": "number",
          "required": true
        }
      },
      "minValue": 70,
      "maxValue": 180
    },
    "standardizeDiagnosisCodes": {
      "enabled": false,
      "parameters": {
        "format": {
          "type": "string",
          "options": ["ICD-10", "ICD-9"]
        }
      },
      "format": "ICD-10"
    },
    "filterOutUnwantedRecords": {
      "enabled": false,
      "parameters": {
        "criteria": {
          "type": "object",
          "properties": {
            "age": {
              "type": "object",
              "properties": {
                "min": {
                  "type": "number"
                },
                "max": {
                  "type": "number"
                }
              }
            },
            "bloodSugarLevel": {
              "type": "object",
              "properties": {
                "max": {
                  "type": "number"
                }
              }
            }
          }
        }
      },
      "criteria": {
        "age": {
          "min": 0,
          "max": 120
        },
        "bloodSugarLevel": {
          "max": 300
        }
      }
    },
    "convertDateFormats": {
      "enabled": false,
      "parameters": {
        "format": {
          "type": "string",
          "options": ["yyyy-MM-dd", "MM/dd/yyyy"]
        }
      },
      "format": "yyyy-MM-dd"
    },
    "trimWhitespace": {
      "enabled": false,
      "parameters": {
        "fields": {
          "type": "array",
          "required": true
        }
      },
      "fields": []
    },
    "categorizeAgeGroups": {
      "enabled": false,
      "parameters": {
        "ageRanges": {
          "type": "array",
          "required": true
        }
      },
      "ageRanges": []
    },
    "customRegexReplacement": {
      "enabled": false,
      "parameters": {
        "pattern": {
          "type": "string",
          "required": true
        },
        "replacement": {
          "type": "string",
          "required": true
        }
      },
      "pattern": "",
      "replacement": ""
    },
    "logCleaningActions": {
      "enabled": false,
      "parameters": {
        "logFormat": {
          "type": "string",
          "options": ["text", "json"]
        }
      },
      "logFormat": "text"
    }
  }
}
